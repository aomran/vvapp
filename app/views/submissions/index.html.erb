<p><%= notice %></p>

<h1>List of Submissions</h1>

<% @submissions.each do |submission| %>
  <ul>
    <li>Artist <%= submission.user.first_name + " " + submission.user.last_name%></li>
    <li>CV <%= submission.cv %></li>
    <li>Artist Statement <%= submission.artist_statement %></li>
    <li>Expo Project <%= submission.expo_project %></li>
    <li>Special Needs <%= submission.special_needs %></li>
    <li>Image List <%= submission.image_list %></li>
  </ul>
<% end %>